{
  "name": "vues",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test": "jest",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@firebase/app-types": "0.x",
    "@firebase/database": "^0.5.25",
    "bootstrap": "^4.5.0",
    "bootstrap-vue": "^2.14.0",
    "core-js": "^3.6.5",
    "firebase": "^7.14.3",
    "jest-serializer-vue": "^2.0.2",
    "jquery": "1.9.1",
    "moment": "^2.25.3",
    "numeral": "^2.0.6",
    "popper.js": "^1.16.0",
    "sinon": "^9.0.2",
    "typescript": "*",
    "vue": "2.6.11",
    "vue-jest": "^3.0.5",
    "vue-print-nb": "^1.5.0",
    "vue-router": "^3.1.6",
    "vue-simple-alert": "^1.1.1",
    "vue-template-es2015-compiler": "1.9.x",
    "vuefire": "^2.2.2",
    "vuelidate": "^0.7.5",
    "vuex": "^3.4.0",
    "vuex-persistedstate": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-router": "^4.3.1",
    "@vue/cli-plugin-unit-jest": "~4.3.0",
    "@vue/cli-plugin-vuex": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^26.0.1",
    "jest-vue-preprocessor": "^1.7.1",
    "jsdom": "^16.2.2",
    "vue-server-renderer": "^2.6.11",
    "vue-template-compiler": "2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
